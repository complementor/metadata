<rdfs:subClassOf rdf:resource="http://m3o.semantic-multimedia.org/ontology/2009/09/16/annotation.owl#AnnotationConcept"/>
<rdfs:subClassOf>
<owl:Restriction>
<owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#classifies"/>
<owl:allValuesFrom rdf:resource="#RecordingDateRegion"/>
</owl:Restriction>
</rdfs:subClassOf>
</owl:Class>

